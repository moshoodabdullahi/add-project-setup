{
  "connector": {
    "name": "local",
    "options": {
      "pattern": [
        "**",
        "!.git/**",
        "!node_modules/**"
      ]
    }
  },
  "extends": [
    "development"
  ],
  "formatters": [
    "stylish"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "button-type": "off"
  }
}